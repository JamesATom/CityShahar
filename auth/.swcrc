{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "decorators": true,
        "dynamicImport": true
      },
      "target": "es2017",
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
      "keepClassNames": true,
      "baseUrl": "./"
    },
    "module": {
      "type": "commonjs"
    },
    "sourceMaps": true,
    "minify": false
  }
  